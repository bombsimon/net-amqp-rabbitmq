listeners.tcp.default = 5672
listeners.ssl.default = 5673
num_acceptors.tcp = 10
num_acceptors.ssl = 10
reverse_dns_lookups = false
loopback_users.guest = true
ssl_options.verify = verify_peer
ssl_options.fail_if_no_peer_cert = true
ssl_options.cacertfile = TRAVIS_DIR/travis/certs/ca_certificate.pem
ssl_options.certfile = TRAVIS_DIR/travis/certs/server_certificate.pem
ssl_options.keyfile = TRAVIS_DIR/travis/certs/server_key.pem
log.console = false
log.console.level = debug
